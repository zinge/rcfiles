alignment top_right
background no
border_width 1
cpu_avg_samples 2
default_color 000000
default_outline_color 000000
default_shade_color 000000
draw_borders no
draw_graph_borders yes
draw_outline no
draw_shades no
use_xft yes
xftfont DejaVu Sans Mono:size=6
gap_x 5
gap_y 60
minimum_size 5 5
net_avg_samples 2
no_buffers yes
out_to_console no
out_to_stderr no
extra_newline no
own_window yes
own_window_class Conky
own_window_type desktop
own_window_transparent yes
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
stippled_borders 0
update_interval 1.0
uppercase no
use_spacer none
show_graph_scale no
show_graph_range no   
double_buffer yes

TEXT
${alignc}${font size=22}${time %H:%M:%S}${font}
${alignc}${time %A} ${time %B} ${time %d}, ${time %Y}
$hr
${color 191970}Uptime:$color $uptime
${color 191970}Frequency (in MHz):$color $freq
${color 191970}CPU Usage:$color $cpu%
${cpubar 10}
${color 191970}CPU1: ${cpugraph cpu1 000000 ffff00}
${color 191970}CPU2: ${cpugraph cpu2 000000 ffff00}
${color 191970}CPU3: ${cpugraph cpu3 000000 ffff00}
${color 191970}CPU4: ${cpugraph cpu4 000000 ffff00}
$hr
${color 191970}Memory && Swap:
${color 191970}RAM Usage:$color $mem/$memmax - $memperc%
${membar 10}
${color 191970}Swap Usage:$color $swap/$swapmax - $swapperc%
${swapbar 10}
$hr
${color 191970}File systems:
 / $color${fs_used /}/${fs_size /} ${fs_bar 6 /}
 /boot $color${fs_used /boot}/${fs_size /boot} ${fs_bar 6 /boot}
 /boot/efi $color${fs_used /boot/efi}/${fs_size /boot/efi} ${fs_bar 6 /boot/efi}
$hr
${color 191970}Networking:
${downspeedgraph enp5s0 000000 ff0000}
DLS:$color ${downspeed enp5s0}/s ${color 191970}${alignr}Total:$color ${totaldown enp5s0} ${color 191970}
${upspeedgraph enp5s0 000000 00ff00}
ULS:$color ${upspeed enp5s0}/s ${color 191970}${alignr}Total:$color ${totalup enp5s0} ${color 191970}
$hr
${color 191970}Processes:$color $processes  ${color 191970}Running:$color $running_processes
${color 191970}Top 10:
${color 191970}Name              PID   CPU%   MEM%
${color ff0000} ${top name 1} ${top pid 1} ${top cpu 1} ${top mem 1}
${color ff0000} ${top name 2} ${top pid 2} ${top cpu 2} ${top mem 2}
${color ff0000} ${top name 3} ${top pid 3} ${top cpu 3} ${top mem 3}
${color ff4500} ${top name 4} ${top pid 4} ${top cpu 4} ${top mem 4}
${color ff4500} ${top name 5} ${top pid 5} ${top cpu 5} ${top mem 5}
${color ff4500} ${top name 6} ${top pid 6} ${top cpu 6} ${top mem 6}
${color ff4500} ${top name 7} ${top pid 7} ${top cpu 7} ${top mem 7}
${color ff4500} ${top name 8} ${top pid 8} ${top cpu 8} ${top mem 8}
${color ff4500} ${top name 9} ${top pid 9} ${top cpu 9} ${top mem 9}
${color ff4500} ${top name 10} ${top pid 10} ${top cpu 10} ${top mem 10}
